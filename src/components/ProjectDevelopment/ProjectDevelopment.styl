  .slider-container
    width (144/rfs)rem
    position absolute
    bottom (20/rfs)rem
    left 0px
    right 0px
    margin auto

    &.not-visible
      visibility: hidden

    div > div > div > div //i hate material-ui in this moment
      &:nth-child(2)
        background-color: blue!important
        border-color: blue!important

      &:nth-child(3)
        background-color: blue !important
