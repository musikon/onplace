  "scripts": {
    "start": "node node_modules/babel-cli/bin/babel-node.js bin/server.js",
    "prod": "pm2 stop onplace && export NODE_ENV=production && rm -rf .tmp && npm install && pm2 start --interpreter babel-node bin/server.js --name=\"onplace\" && pm2 logs onplace",
    "prod_without_stop": "export NODE_ENV=production && rm -rf .tmp && npm install && pm2 start --interpreter babel-node bin/server.js --name=\"onplace\" && pm2 logs onplace",
    "prodplug": "pm2 stop onplace-plug && export NODE_ENV=production PORT=3223 && rm -rf .tmp && npm install && pm2 start --interpreter babel-node bin/server.js --name=\"onplace-plug\" && pm2 logs onplace-plug",
    "prod_win": "set NODE_ENV=production && npm start",
    "lint": "eslint ./src --quiet"
  },
